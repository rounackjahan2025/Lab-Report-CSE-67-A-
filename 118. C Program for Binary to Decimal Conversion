#include <stdio.h>
#include <math.h>

int main() {
    long long binary;
    int decimal = 0, base = 0, rem;

    printf("Enter binary number: ");
    scanf("%lld", &binary);

    while(binary > 0) {
        rem = binary % 10;
        decimal += rem * pow(2, base++);
        binary /= 10;
    }

    printf("Decimal: %d\n", decimal);
    return 0;
}

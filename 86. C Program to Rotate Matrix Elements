#include <stdio.h>

int main() {
    int n;
    printf("Enter size of square matrix: ");
    scanf("%d",&n);

    int mat[n][n];
    printf("Enter elements:\n");
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&mat[i][j]);

    int temp = mat[0][0];
    for(int i=0;i<n-1;i++) mat[i][0]=mat[i+1][0];
    for(int j=0;j<n-1;j++) mat[n-1][j]=mat[n-1][j+1];
    for(int i=n-1;i>0;i--) mat[i][n-1]=mat[i-1][n-1];
    for(int j=n-1;j>1;j--) mat[0][j]=mat[0][j-1];
    mat[0][1]=temp;

    printf("Rotated matrix:\n");
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++) printf("%d ",mat[i][j]);
        printf("\n");
    }
    return 0;
}

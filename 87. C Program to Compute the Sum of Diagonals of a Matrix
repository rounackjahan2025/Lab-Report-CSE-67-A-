#include <stdio.h>

int main() {
    int n, sum=0;
    printf("Enter size of square matrix: ");
    scanf("%d",&n);

    int mat[n][n];
    printf("Enter elements:\n");
    for(int i=0;i<n;i++)
        for(int j=0;j<n;j++)
            scanf("%d",&mat[i][j]);

    for(int i=0;i<n;i++){
        sum += mat[i][i];         // Primary diagonal
        if(i!=n-1-i) sum += mat[i][n-1-i]; // Secondary diagonal
    }

    printf("Sum of diagonals = %d\n", sum);
    return 0;
}

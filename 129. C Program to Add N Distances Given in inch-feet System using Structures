#include <stdio.h>

struct Distance {
    int feet;
    float inch;
};

int main() {
    int n;
    printf("Enter number of distances: ");
    scanf("%d", &n);

    struct Distance d[n], sum = {0, 0};

    for(int i=0;i<n;i++) {
        printf("Enter feet and inches: ");
        scanf("%d %f", &d[i].feet, &d[i].inch);
        sum.feet += d[i].feet;
        sum.inch += d[i].inch;
    }

    sum.feet += (int)(sum.inch / 12);
    sum.inch = fmod(sum.inch, 12);

    printf("Total Distance = %d'-%.2f\"\n", sum.feet, sum.inch);
    return 0;
}

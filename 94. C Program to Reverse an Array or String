#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    printf("Enter string: ");
    scanf(" %[^\n]s", str);

    int len=strlen(str);
    for(int i=0;i<len/2;i++){
        char t=str[i]; str[i]=str[len-1-i]; str[len-1-i]=t;
    }
    printf("Reversed string: %s\n", str);
    return 0;
}

#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100];
    printf("Enter first string: ");
    scanf("%s", a);
    printf("Enter second string: ");
    scanf("%s", b);

    int result = strcmp(a, b);

    if(result == 0)
        printf("Both strings are equal.\n");
    else if(result < 0)
        printf("\"%s\" comes before \"%s\" lexicographically.\n", a, b);
    else
        printf("\"%s\" comes after \"%s\" lexicographically.\n", a, b);

    return 0;
}

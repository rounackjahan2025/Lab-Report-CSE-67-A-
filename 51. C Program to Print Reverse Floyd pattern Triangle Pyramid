#include <stdio.h>

int main() {
    int n, num = 1;
    printf("Enter number of rows: ");
    scanf("%d", &n);

    int total = n*(n+1)/2;

    for(int i = 1; i <= n; i++) {
        for(int j = 1; j <= i; j++) {
            printf("%d ", total);
            total--;
        }
        printf("\n");
    }

    return 0;
}

#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[200];
    int alphabets[26]={0};
    printf("Enter string: ");
    scanf(" %[^\n]s", str);

    for(int i=0;i<strlen(str);i++){
        if(isalpha(str[i]))
            alphabets[tolower(str[i])-'a']=1;
    }

    int pangram=1;
    for(int i=0;i<26;i++){
        if(alphabets[i]==0){
            pangram=0;
            break;
        }
    }

    if(pangram) printf("Pangram\n");
    else printf("Not Pangram\n");
    return 0;
}

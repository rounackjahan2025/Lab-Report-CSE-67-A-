#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter elements: ");
    for(int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    int sum = 0;
    for(int i = 0; i < n; i++)
        sum += arr[i];

    printf("Average = %.2f\n", (float)sum/n);
    return 0;
}

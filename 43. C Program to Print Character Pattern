#include <stdio.h>

int main() {
    int rows;
    char ch = 'A';
    printf("Enter number of rows: ");
    scanf("%d", &rows);

    for(int i = 1; i <= rows; i++) {
        for(int j = 1; j <= i; j++) {
            printf("%c ", ch);
            ch++;
            if(ch > 'Z') ch = 'A';
        }
        printf("\n");
    }

    return 0;
}

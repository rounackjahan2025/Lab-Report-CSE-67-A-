#include <stdio.h>

int main() {
    int n;
    printf("Enter number of elements: ");
    scanf("%d",&n);

    int arr[n];
    printf("Enter elements in sorted order: ");
    for(int i=0;i<n;i++) scanf("%d",&arr[i]);

    int j = 0;
    for(int i=1;i<n;i++) {
        if(arr[i]!=arr[j]) arr[++j]=arr[i];
    }

    printf("Array after removing duplicates: ");
    for(int i=0;i<=j;i++) printf("%d ",arr[i]);
    return 0;
}

#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main() {
    char str[200];
    printf("Enter string: ");
    scanf(" %[^\n]s", str);

    printf("First letters: ");
    if(str[0]!=' ') printf("%c ",str[0]);
    for(int i=1;i<strlen(str);i++){
        if(str[i-1]==' ' && str[i]!=' ')
            printf("%c ",str[i]);
    }
    return 0;
}

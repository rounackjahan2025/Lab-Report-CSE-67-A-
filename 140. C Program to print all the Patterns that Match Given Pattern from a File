#include <stdio.h>
#include <string.h>

int main() {
    char pattern[50], line[200];
    FILE *f = fopen("text.txt", "r");
    if(!f){ printf("File not found.\n"); return 1; }

    printf("Enter pattern: ");
    scanf("%s", pattern);

    while(fgets(line, sizeof(line), f))
        if(strstr(line, pattern))
            printf("%s", line);

    fclose(f);
    return 0;
}

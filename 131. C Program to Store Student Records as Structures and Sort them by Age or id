#include <stdio.h>

struct Student {
    int id, age;
    char name[50];
};

int main() {
    int n;
    printf("Enter number of students: ");
    scanf("%d", &n);
    struct Student s[n], temp;

    for(int i=0;i<n;i++)
        scanf("%d %d %s", &s[i].id, &s[i].age, s[i].name);

    for(int i=0;i<n-1;i++)
        for(int j=i+1;j<n;j++)
            if(s[i].age > s[j].age) {
                temp=s[i]; s[i]=s[j]; s[j]=temp;
            }

    for(int i=0;i<n;i++)
        printf("%d %d %s\n", s[i].id, s[i].age, s[i].name);
    return 0;
}

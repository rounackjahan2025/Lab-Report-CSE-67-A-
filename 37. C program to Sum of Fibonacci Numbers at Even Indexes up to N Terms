#include <stdio.h>

int main() {
    int n;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    int t1 = 0, t2 = 1, nextTerm, sum = 0;
    for(int i = 0; i < n; i++) {
        if(i % 2 == 0)
            sum += t1;
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
    }

    printf("Sum of Fibonacci numbers at even indexes = %d\n", sum);
    return 0;
}

#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    printf("Enter string: ");
    scanf(" %[^\n]s", str);

    int freq[256]={0};
    for(int i=0;i<strlen(str);i++) freq[(int)str[i]]++;
    
    for(int i=0;i<strlen(str);i++){
        if(freq[(int)str[i]]==1){
            printf("First non-repeating character: %c\n", str[i]);
            break;
        }
    }
    return 0;
}

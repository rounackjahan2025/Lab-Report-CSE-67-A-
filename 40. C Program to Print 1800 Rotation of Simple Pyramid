#include <stdio.h>

int main() {
    int rows;
    printf("Enter number of rows: ");
    scanf("%d", &rows);

    for(int i = 1; i <= rows; i++) {
        for(int j = 1; j <= i - 1; j++)
            printf(" ");
        for(int k = 1; k <= rows - i + 1; k++)
            printf("*");
        printf("\n");
    }

    return 0;
}

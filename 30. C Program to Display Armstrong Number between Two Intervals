#include <stdio.h>
#include <math.h>

int main() {
    int lower, upper, num, sum, temp, remainder, digits;
    printf("Enter two intervals: ");
    scanf("%d %d", &lower, &upper);

    printf("Armstrong numbers between %d and %d:\n", lower, upper);
    for(num = lower; num <= upper; num++) {
        sum = 0;
        temp = num;
        digits = 0;

        int temp2 = temp;
        while(temp2 != 0) {
            digits++;
            temp2 /= 10;
        }

        while(temp != 0) {
            remainder = temp % 10;
            sum += pow(remainder, digits);
            temp /= 10;
        }

        if(sum == num)
            printf("%d ", num);
    }

    return 0;
}
